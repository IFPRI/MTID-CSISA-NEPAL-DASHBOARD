*************** Seasonality of real prices of 10 commodities in six markets**********

. disp ""


. foreach mkt in "Birendranagar" "Nepalgunj" "Kathmandu" ///
>                "Birgunj"       "Dhankuta"   "Biratnagar" {
  2.         preserve
  3.         keep if market=="`mkt'"
  4.         disp ""
  5.         disp "Seasonal price indices for `mkt'"
  6.         table month commodity, stat(count sindex) stat(mean sindex) stat(sd sindex) ///
>           nformat(%5.1f) totals(commodity)
  7.         disp ""
  8.         egen maxprice = max(sindex), by(commodity year)
  9.         egen minprice = min(sindex), by(commodity year)
 10.         gen seasonality = maxprice-minprice
 11.         disp ""
 12.         disp "Measure of seasonality for `mkt' (max-min/avg)"
 13.         table commodity, stat(mean seasonality) stat(sd seasonality) stat(min seasonality) ///
>            stat(max seasonality) nototal nformat(%5.1fc)
 14.         restore  
 15. }
